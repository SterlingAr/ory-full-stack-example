[
  {
    "id": "${ORGANIZATION}-preflight-check-${ENVIROMENT}",
    "description": "Allow preflight requests for organization: ${ORGANIZATION}",
    "upstream": {
      "url": "${RESOURCE_SERVER}"
    },
    "match": {
      "url": "${OATHKEEPER_PROXY_URL}<.+>",
      "methods": [
        "OPTIONS"
      ]
    },
    "authenticators": [
      {
        "handler": "noop"
      }
    ],
    "authorizer": {
      "handler": "allow"
    },
    "credentials_issuer": {
      "handler": "noop"
    }
  }
]