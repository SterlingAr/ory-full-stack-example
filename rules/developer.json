[
  {
    "id": "keto-administration",
    "upstream": {
      "url": "http://keto-service"
    },
    "match": {
      "url": "http://keto.marius-cv.com/<(.+)>",
      "methods": [
        "GET",
        "POST",
        "HEAD",
        "OPTIONS",
        "PUT",
        "PATCH"
      ]
    },
    "authenticators": [{
      "handler": "oauth2_introspection",
      "config": {
        "required_scope": [
          "keto"
        ]
      }
    }],
    "authorizer": {
      "handler": "keto_engine_acp_ory",
      "config": {
        "required_action": "keto:any",
        "required_resource": "keto:any"
      }
    },
    "credentials_issuer": {
      "handler": "id_token"
    }
  },
